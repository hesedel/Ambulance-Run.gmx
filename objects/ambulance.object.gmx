        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(!vehicle_onExit(ambulance)) {

    // prevent ambulance from veering off the road to the right
    if(!place_meeting(x + sprite_width,y,roads)) {
        x = instance_place(x,y,roads).x +
            instance_place(x,y,roads).sprite_width -
            (sprite_width / 2);
    }

    // prevent ambulance from veering off the road to the left
    if(!place_meeting(x - sprite_width,y,roads)) {
        x = instance_place(x,y,roads).x +
            (sprite_width / 2);
    }

    // exit entrance
    if(instance_place(x,y,exits)) {
        if(
            instance_place(x,y,exits_ne) ||
            instance_place(x,y,exits_se)
        ) {
            exit_entrance = x -
                instance_place(x,y,exits).x -
                16;
        }
        if(
            instance_place(x,y,exits_nw) ||
            instance_place(x,y,exits_sw)
        ) {
            exit_entrance = instance_place(x,y,exits).x +
                instance_place(x,y,exits).sprite_width -
                x -
                16;
        }
    }

}

// exits
vehicle_exitPivot(ambulance);

// turning siren off
if(siren_timeout &gt; 0)
    siren_timeout--;

</string>
